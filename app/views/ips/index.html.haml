.container
  %table.manasyPanel.table
    %tr
      %th Name
      %th
      %th
        
    - @ips.each do |ip|
      %tr
        %td= ip.name
        %td
          = link_to edit_ip_path(ip) do
            %span.glyphicon.glyphicon-edit
        %td
          = link_to ip, :method => :delete, :data => { :confirm => 'Esta seguro?' } do
            %span.glyphicon.glyphicon-trash
